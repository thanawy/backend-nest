{
  "compilerOptions": {
    "baseUrl": "./src",
    "rootDir": "./src",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "dist/src",
    "incremental": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@auth/*": ["auth/*"], // Maps "@auth/" to "auth/"
      "@answers/*": ["answers/*"], // Maps "@models/" to "models/"
      "@units/*": ["units/*"], // Maps "@models/" to "models/"
      "@classes/*": ["classes/*"], // Maps "@models/" to "models/"
      "@lessons/*": ["lessons/*"], // Maps "@models/" to "models/"
      "@orders/*": ["orders/*"], // Maps "@models/" to "models/"
      "@onboarding-screen/*": ["onboarding-screen/*"], // Maps "@models/" to "models/
      "@choice/*": ["choice/*"], // Maps "@choice/" to "choice/"
      "@collections/*": ["collections/*"], // Maps "@models/" to "models/"
      "@database/*": ["database/*"], // Maps "@models/" to "models/"
      "@plans/*": ["plans/*"], // Maps "@models/" to "models/"
      "@programs/*": ["programs/*"], // Maps "@models/" to "models/"
      "@questions/*": ["questions/*"], // Maps "@models/" to "models/"
      "@subjects/*": ["subjects/*"], // Maps "@models/" to "models/"
      "@subscriptions/*": ["subscriptions/*"], // Maps "@models/" to "models/"
      "@tags/*": ["tags/*"], // Maps "@models/" to "models/"
      "@users/*": ["users/*"], // Maps "@models/" to "models/"
      "@payments/*": ["payments/*"], // Maps "@models/" to "models/"
      "@chapters/*": ["chapters/*"], // Maps "@models/" to "models/"
      "@config/*": ["config/*"],
      "@mailer/*": ["mailer/*"],
      "@verification/*": ["auth/verification/*"]
    }
  }
}
