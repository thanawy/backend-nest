{
  "name": "backend-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register src/tools/typeorm.custom-cli.ts",
    "typeorm-extension": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs",
    "migration:generate": "yarn run typeorm migration:generate --dataSource src/database/data-source.ts src/database/migrations/migration",
    "migration:run": "yarn run typeorm migration:run --dataSource src/database/data-source.ts",
    "migration:revert": "yarn run typeorm migration:revert --dataSource src/database/data-source.ts",
    "seed:create": "yarn run typeorm-extension seed:create --name src/database/seeds/new.seeder.ts ",
    "seed:run": "yarn run typeorm-extension seed:run --dataSource src/database/data-source.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/passport": "^2.4.0",
    "@fastify/secure-session": "^7.1.0",
    "@fastify/session": "^10.7.0",
    "@mgcrea/fastify-request-logger": "^1.5.0",
    "@mgcrea/pino-pretty-compact": "^1.3.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.3.1",
    "@nestjs/typeorm": "^10.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-local": "^1.0.38",
    "@types/winston": "^2.4.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bcrypt-ts": "^5.0.1",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "connect-typeorm": "^2.0.0",
    "crypto-js": "^4.2.0",
    "ejs": "^3.1.10",
    "faker": "^6.6.6",
    "handlebars": "^4.7.8",
    "ioredis": "^5.3.2",
    "nest-winston": "^1.9.4",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "preview-email": "^3.0.19",
    "pug": "^3.0.2",
    "random": "^3.0.6",
    "random-js": "^2.1.0",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-store": "^2.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}
